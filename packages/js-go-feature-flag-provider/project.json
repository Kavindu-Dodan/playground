{
  "root": "packages/js-go-feature-flag-provider",
  "sourceRoot": "packages/js-go-feature-flag-provider/src",
  "projectType": "library",
  "targets": {
    "build": {
      "executor": "@nrwl/node:webpack",
      "outputs": ["{options.outputPath}"],
      "options": {
        "outputPath": "dist/packages/js-go-feature-flag-provider",
        "main": "packages/js-go-feature-flag-provider/src/index.ts",
        "tsConfig": "packages/js-go-feature-flag-provider/tsconfig.lib.json"
      },
      "configurations": {
        "production": {
          "optimization": true,
          "extractLicenses": true,
          "inspect": false
        }
      }
    },
    "lint": {
      "executor": "@nrwl/linter:eslint",
      "outputs": ["{options.outputFile}"],
      "options": {
        "lintFilePatterns": ["packages/js-go-feature-flag-provider/**/*.ts"]
      }
    },
    "test": {
      "executor": "@nrwl/jest:jest",
      "outputs": ["coverage/packages/js-go-feature-flag-provider"],
      "options": {
        "jestConfig": "packages/js-go-feature-flag-provider/jest.config.js",
        "passWithNoTests": true
      }
    }
  },
  "tags": []
}
